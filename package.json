  "homepage": "https://github.com/Caserage/deb#readme",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.41",
    "@types/node": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.5.1",
